{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "infra.js"
  },
  "scripts": {
    "cdk": "cdk",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint .",
    "fix:lint": "eslint . --fix",
    "preinfra:deploy": "tsx hooks/pre-deploy.hook.ts",
    "postinfra:deploy": "tsx hooks/post-deploy.hook.ts",
    "preinfra:destroy": "tsx hooks/pre-destroy.hook.ts",
    "postinfra:destroy": "tsx hooks/post-destroy.hook.ts",
    "cdk:deploy:integration": "cdk deploy IntegrationStack --require-approval never",
    "deploy:integration": "npm run preinfra:deploy && npm run cdk:deploy:integration && npm run postinfra:deploy",
    "destroy:integration": "npm run preinfra:destroy && npm run cdk:destroy:integration && npm run postinfra:destroy",
    "cdk:destroy:integration": "cdk destroy IntegrationStack",
    "cdk:deploy:ephemeral": "cdk deploy EphemeralStack --require-approval never",
    "deploy:ephemeral": "npm run preinfra:deploy && npm run cdk:deploy:ephemeral && npm run postinfra:deploy",
    "cdk:destroy:ephemeral": "cdk destroy EphemeralStack --force",
    "destroy:ephemeral": "npm run preinfra:destroy && npm run cdk:destroy:ephemeral && npm run postinfra:destroy",
    "cdk:deploy:production": "cdk deploy ProductionStack --require-approval never",
    "deploy:production": "npm run preinfra:deploy && npm run cdk:deploy:production && npm run postinfra:deploy",
    "cdk:destroy:production": "cdk destroy ProductionStack --force",
    "destroy:production": "npm run preinfra:destroy && npm run cdk:destroy:production && npm run postinfra:destroy"
  },
  "devDependencies": {
    "@types/mime-types": "catalog:",
    "@types/node": "catalog:",
    "@types/openapi-to-postmanv2": "^5.0.0",
    "aws-cdk": "2.1033.0",
    "dotenv": "catalog:",
    "esbuild": "0",
    "jest": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "@eslint/js": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-unused-imports": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "@aws-lambda-powertools/commons": "catalog:",
    "@aws-lambda-powertools/logger": "catalog:",
    "@aws-sdk/client-cloudformation": "catalog:",
    "@aws-sdk/client-cloudfront": "catalog:",
    "@aws-sdk/client-cognito-identity-provider": "catalog:",
    "@aws-sdk/client-dynamodb": "catalog:",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/client-secrets-manager": "catalog:",
    "@aws-sdk/client-ssm": "catalog:",
    "@aws-sdk/lib-dynamodb": "catalog:",
    "aws-cdk-lib": "2.230.0",
    "axios": "catalog:",
    "axios-retry": "catalog:",
    "constructs": "^10.0.0",
    "mime-types": "catalog:",
    "openapi-to-postmanv2": "^5.0.1",
    "s3-sync-client": "^4.3.1"
  }
}
